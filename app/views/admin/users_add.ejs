<% include header %>
    <div class="main-wrap">

        <div class="crumb-wrap">
            <div class="crumb-list"><i class="icon-font"></i><a href="/admin/index">首页</a><span class="crumb-step">&gt;</span><a class="crumb-name" href="/admin/categorys">栏目管理</a><span class="crumb-step">&gt;</span><span>新增栏目</span></div>
        </div>
        <div class="result-wrap">
            <div class="result-content">
                <form action="/admin/categorys_add_do" method="post" id="myform" name="myform" >
                    <table class="insert-tab" width="100%">
                        <tbody>
                            <tr>
                                <th><i class="require-red">*</i>栏目名称：</th>
                                <td>
                                    <input class="common-text required" id="title" name="categorys_name" size="50" value="test" type="text">
                                </td>
                            </tr>
			    <tr>
                                <th><i class="require-red">*</i>栏目链接：</th>
                                <td>
                                    <input class="common-text required" id="title" name="categorys_url" size="50" value="test" type="text">
                                </td>
                            </tr>
			    <tr>
                                <th>栏目关键字：</th>
                                <td>
                                    <input class="common-text" id="title" name="categorys_keyword" size="50" value="test" type="text">
                                </td>
                            </tr>
			    <tr>
                                <th>栏目描述：</th>
                                <td>
                                    <input class="common-text" id="title" name="categorys_discription" size="50" value="test" type="text">
                                </td>
                            </tr>
                            
                            <tr>
                                <th></th>
                                <td>
                                    <input class="btn btn-primary btn6 mr10" value="提交" type="submit">
                                    <input class="btn btn6" onclick="history.go(-1)" value="返回" type="button">
                                </td>
                            </tr>
                        </tbody></table>
                </form>
            </div>
        </div>

    </div>
